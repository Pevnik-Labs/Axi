// An attempt at a propositional logic benchmark for various AIs.

// default/4o(?) one shot.
// DeepSeek V3 one-shot.
theorem iff-not-nand :
  ~ (P <--> Q) --> ~ P \/ ~ Q
proof

qed

// 4o one shot.
// DeepSeek V3 two shot.
theorem and-or-distrib :
  P /\ (Q \/ R) <--> (P /\ Q) \/ (P /\ R)
proof

qed

// 4o one shot, but with a more complicated proof.
// DeepSeek V3 two shot.
theorem de-morgan :
  ~ (P /\ Q) --> ~ P \/ ~ Q
proof

qed

// 4o two shot.
// DeepSeek V3 one-shot.
theorem de-morgan-conv :
  ~ P \/ ~ Q --> ~ (P /\ Q)
proof

qed

// DeepSeek V3 two shot.
theorem de-morgan-and :
  ~ (P \/ Q) <--> ~ P /\ ~ Q
proof

qed

theorem impl-and-distrib :
  (P --> Q /\ R) <--> (P --> Q) /\ (P --> R)
proof

qed

theorem impl-or-distrib :
  ((P \/ Q) --> R) <--> (P --> R) /\ (Q --> R)
proof

qed

// Very hard.
theorem iff-not-conv :
  (~ P <--> ~ Q) --> (P <--> Q)
proof

qed